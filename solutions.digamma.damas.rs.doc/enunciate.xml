<?xml version="1.0" encoding="UTF-8"?>
<enunciate xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:noNamespaceSchemaLocation="http://enunciate.webcohesion.com/schemas/enunciate-2.13.0.xsd">

    <api-classes>
        <include pattern="solutions.digamma.damas.rs.**" />
        <include pattern="solutions.digamma.damas.api.**" />
        <exclude pattern="solutions.digamma.damas.rs.doc"/>
    </api-classes>

    <modules disabledByDefault="true">
        <!-- provider modules -->
        <jaxb disabled="false" />
        <jackson collapse-type-hierarchy="true" dateFormat="string" disabled="false"/>
        <jaxrs disabled="false" />
        <!-- non-provider modules -->
        <docs disabled="false" />
        <swagger basePath="/rest" disabled="false" />
    </modules>

</enunciate>
