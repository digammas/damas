{
  "storage": {
    "persistence": {
      "type": "file",
      "path": "repository/storage"
    }
  },
  "indexProviders": {
    "lucene": {
      "classname": "lucene",
      "directory": "repository/index"
    }
  },
  "indexes": {
  },
  "textExtraction": {
    "extractors": {
      "tikaExtractor": {
        "name": "Tika content-based extractor",
        "classname": "tika"
      }
    }
  },
  "security" : {
    "providers" : [{
      "name" : "System Authentication Provider",
      "classname" : "solutions.digamma.damas.jcr.providers.CustomSecurityProvider",
      "policy" : "damas-system"
    }, {
      "name" : "User Authentication Provider",
      "classname" : "solutions.digamma.damas.jcr.providers.CustomSecurityProvider",
      "policy" : "damas-user"
    }]
  },
  "node-types" : ["repository/cnd/damas.cnd"]
}
